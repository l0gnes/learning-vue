{
  "name": "learning_vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "bootstrap": "~4.5.3",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "glob-parent": ">=5.1.2",
    "node-forge": ">=1.0.0",
    "portal-vue": "^2.1.7",
    "postcss": ">=8.2.13",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-style-loader": "^4.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "indent": [
        0,
        "space"
      ],
      "no-ident": 0,
      "no-tabs": 0,
      "max-len": 0,
      "key-spacing": 0,
      "comma-dangle": 0,
      "no-unused-vars" : 0,
      "prefer-const" : 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
